			<xs:element name="Upper_Right_Corner" type="ee:VE_Carto_Location_2D_Type">
				<xs:annotation>
					<xs:documentation>see Upper_Left_Corner for information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Lower_Left_Corner" type="ee:VE_Carto_Location_2D_Type">
				<xs:annotation>
					<xs:documentation>see Upper_Left_Corner for information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Lower_Right_Corner" type="ee:VE_Carto_Location_2D_Type">
				<xs:annotation>
					<xs:documentation>see Upper_Left_Corner for information.</xs:documentation>
				</xs:annotation>
