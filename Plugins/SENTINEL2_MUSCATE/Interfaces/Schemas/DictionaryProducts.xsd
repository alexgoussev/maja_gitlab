	<xs:complexType name="VE_L1_Product_Organization_Type">
		<xs:annotation>
			<xs:documentation>Public description of organization of a published product.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_Published_Product_Organization_Type">
				<xs:sequence>
					<xs:element name="List_of_Annex_Files">
						<xs:annotation>
							<xs:documentation>For L1 product, annex files are : 
- Cloud Altitudes (CLA) ;
- Solar angles Grid (SOL) ;
- Viewing angles Grid (VIE) ;</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Annex_File" minOccurs="0" maxOccurs="4">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="ee:VE_Product_Item_Reference_Type">
												<xs:attribute name="sk" type="ee:VE_Product_Annex_Code_Type" use="required">
													<xs:annotation>
														<xs:documentation>annex code : CLA for l1 products</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="count" use="required">
								<xs:simpleType>
									<xs:restriction base="ee:Integer_Type">
										<xs:minInclusive value="0"/>
										<xs:maxInclusive value="4"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element name="Private_Data_File" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Link to private data file. This link is only required in the internal version and must be deleted before the publication of the product.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="ee:VE_Product_Item_Reference_Type"/>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="List_of_Extension_Data_Files" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Future extensions</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Extension_Data_File" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="ee:VE_Product_Item_Reference_Type">
												<xs:attribute name="sn" use="required">
													<xs:simpleType>
														<xs:restriction base="ee:Integer_Type">
															<xs:minInclusive value="1"/>
															<xs:maxInclusive value="100"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="count" use="required">
								<xs:simpleType>
									<xs:restriction base="ee:Integer_Type">
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--Elements-->
	<xs:complexType name="VE_Quality_Index_Type">
		<xs:annotation>
			<xs:documentation>Quality indexes</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code" type="ee:VE_Quality_Index_Code_Type">
				<xs:annotation>
					<xs:documentation>The qualtity index code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Value">
				<xs:annotation>
					<xs:documentation>Quality index value in string.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="ee:String_Type">
							<xs:attribute name="unit" type="ee:String_Type"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Band_Code" type="ee:VE_Band_Code_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For the band specific indexes</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VE_List_of_Optionnal_Aks_Type">
		<xs:annotation>
			<xs:documentation>Absolute radiometric coefficients (one by band)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Ak" minOccurs="0" maxOccurs="12">
				<xs:annotation>
					<xs:documentation>absolute radiometric calibration coefficient for the current band. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="ee:VE_Decimal_Ak_Type">
							<xs:attribute name="sk" type="ee:VE_Band_Code_Type" use="required">
								<xs:annotation>
									<xs:documentation>band code</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="count" use="required">
			<xs:annotation>
				<xs:documentation>number of bands</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="ee:Integer_Type">
					<xs:minInclusive value="0"/>
					<xs:maxInclusive value="12"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<!--Decimal values-->
	<xs:complexType name="VE_Decimal_Solar_Angle_Type" mixed="false">
		<xs:annotation>
			<xs:documentation>A solar angle</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Azimuth" type="ee:VE_Decimal_Azimuth_Type"/>
			<xs:element name="Zenith" type="ee:VE_Decimal_Angle_Type"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VE_Decimal_Azimuth_Type" mixed="false">
		<xs:simpleContent>
			<xs:restriction base="ee:VE_Decimal_Angle_Type">
				<xs:minInclusive value="-360"/>
				<xs:maxInclusive value="360"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="VE_Decimal_Solar_Angle_Information_Type" mixed="false">
		<xs:annotation>
			<xs:documentation>All the solar angles informations</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Useful_Image">
				<xs:annotation>
					<xs:documentation>Solar Angle in useful image corners and center</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Upper_Left_Corner" type="ee:VE_Decimal_Solar_Angle_Type"/>
						<xs:element name="Upper_Right_Corner" type="ee:VE_Decimal_Solar_Angle_Type"/>
						<xs:element name="Lower_Left_Corner" type="ee:VE_Decimal_Solar_Angle_Type"/>
						<xs:element name="Lower_Right_Corner" type="ee:VE_Decimal_Solar_Angle_Type"/>
						<xs:element name="Image_Center" type="ee:VE_Decimal_Solar_Angle_Type"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Product" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Solar Angle in product corners. Only required when the product is projected (published products).</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Upper_Left_Corner" type="ee:VE_Decimal_Solar_Angle_Type"/>
						<xs:element name="Upper_Right_Corner" type="ee:VE_Decimal_Solar_Angle_Type"/>
						<xs:element name="Lower_Left_Corner" type="ee:VE_Decimal_Solar_Angle_Type"/>
						<xs:element name="Lower_Right_Corner" type="ee:VE_Decimal_Solar_Angle_Type"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VE_Decimal_Viewing_Angle_Type" mixed="false">
		<xs:annotation>
			<xs:documentation>(cf . pointing direction ?)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Azimuth" type="ee:VE_Decimal_Azimuth_Type"/>
			<xs:element name="Zenith" type="ee:VE_Decimal_Angle_Type"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VE_Decimal_Angle_Type" mixed="false">
		<xs:annotation>
			<xs:documentation>Angle Type with a maximum of 6 fraction digits</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="ee:Angle_Type">
				<xs:fractionDigits value="6"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="VE_Decimal_Geo_Coord_Type" mixed="false">
		<xs:annotation>
			<xs:documentation>Lat Long Coord Type with a maximum of 9 fraction digits</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="ee:Angle_Type">
				<xs:fractionDigits value="9"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="VE_Decimal_Ak_Type" mixed="false">
		<xs:annotation>
			<xs:documentation>Ak Type with a maximum of 5 fraction digits</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="ee:Ak_Type">
				<xs:fractionDigits value="5"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="Ak_Type" mixed="false">
		<xs:simpleContent>
			<xs:extension base="ee:Decimal_Type"/>
		</xs:simpleContent>
	</xs:complexType>
</xs:schema>
