		<xs:annotation>
			<xs:documentation>A geographic coverage with sampling information</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="ee:VE_Geo_Ground_Coverage_Type">
				<xs:sequence>
					<xs:element name="Sampling_Interval">
						<xs:annotation>
							<xs:documentation>Sampling interval of the geo-referenced image (in row and in column) expressed in degrees in "Coordinate_Reference_System".</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="By_Line" type="ee:VE_Decimal_Geo_Coord_Type"/>
								<xs:element name="By_Column" type="ee:VE_Decimal_Geo_Coord_Type"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VE_Carto_Product_Coverage_Type" mixed="false">
		<xs:annotation>
			<xs:documentation>A cartographic coverage with sampling information</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="ee:VE_Carto_Ground_Coverage_Type">
				<xs:sequence>
					<xs:element name="Sampling_Interval">
						<xs:annotation>
							<xs:documentation>Sampling interval of the geo-referenced image (in row and in column) expressed in meters in "Coordinate_Reference_System".</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="By_Line" type="ee:Distance_Type"/>
								<xs:element name="By_Column" type="ee:Distance_Type"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ================= -->
	<!--Product information-->
	<!-- ================= -->
	<xs:simpleType name="VE_Product_Level_Type">
		<xs:annotation>
			<xs:documentation>The product level indication.
- INVENTORIED
- L0-EXTENDED
- L0
- L1A
- L1
- L2
- L3
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ee:String_Type">
			<xs:enumeration value="INVENTORIED"/>
			<xs:enumeration value="L0-EXTENDED"/>
			<xs:enumeration value="L0"/>
			<xs:enumeration value="L1A"/>
			<xs:enumeration value="L1"/>
			<xs:enumeration value="L2"/>
			<xs:enumeration value="L3"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Generic-->
	<xs:complexType name="VE_Generic_Product_Information_Type">
		<xs:annotation>
			<xs:documentation>generic public description of a product</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Product_Level">
				<xs:annotation>
					<xs:documentation>Comprehensible product level : INVENTORIED, L0, L0-EXTENDED, L1A, L1, L2, L3</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ee:VE_Product_Level_Type"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Acquisition_Date_Time" type="ee:UTC_Date_Time_Type">
				<xs:annotation>
					<xs:documentation>product data acquisition date-time. For L3 products is the first acquisition date/time.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Acquisition_Orbit_Number" type="ee:VE_Absolute_Orbit_Number_Or_Nil_Reason_NA_With_Unit_Type">
				<xs:annotation>
					<xs:documentation>Acquisition absolute orbit. For  L3 products is the first orbit number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Processing_Information" type="ee:VE_Processing_Type">
				<xs:annotation>
					<xs:documentation>processing informations : date/time and GIPP files</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Web_Site_URL" type="ee:String_Type">
				<xs:annotation>
					<xs:documentation>reference to the Internet site of the VPDS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="List_of_Quality_Indexes">
				<xs:annotation>
					<xs:documentation>Quality indexes
Quality index = value with sk=the code of the quality index (more accessible with xml parser)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Quality_Index" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Quality_Index_Type">
										<xs:attribute name="sn" use="required">
											<xs:simpleType>
												<xs:restriction base="ee:Integer_Type">
													<xs:minInclusive value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Useful_Image_Geo_Coverage" type="ee:VE_Useful_Image_Geo_Coverage_Type">
				<xs:annotation>
					<xs:documentation>Geographic coordinates of the useful part of the product (or image).
The 12 bands may not cover exactly the same ground area, those corners will be computed such as representing the largest ground area imaged by at least one band.
For published products : contains also the same information expressed in image coordinates (lines, columns)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="List_of_Band_Central_Wavelength" type="ee:VE_List_of_Band_Central_Wavelengths_Type">
				<xs:annotation>
					<xs:documentation>wave length of each band</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Solar_Angles" type="ee:VE_Solar_Angle_Information_Type">
				<xs:annotation>
					<xs:documentation>Information relative to solar angles.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="List_of_Viewing_Angles" type="ee:VE_List_of_Product_Viewing_Angles_Informations_Type">
				<xs:annotation>
					<xs:documentation>Information relative to viewing angles.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Product_Sampling">
				<xs:annotation>
					<xs:documentation>Product sampling in meters.
- For inv and sys products the product sampling is the along track (By_Line) and along detector (By_Column) resolutions.
- For the published products the product_sampling is the Sampling_Interval (Geo_Referencing_Information)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="By_Line" type="ee:Distance_Type"/>
						<xs:element name="By_Column" type="ee:Distance_Type"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Viewing_Angles_In_Satellite_Frame" type="ee:VE_Viewing_Angles_In_Satellite_Frame_Type">
				<xs:annotation>
					<xs:documentation>X,Y, and XY at the center of the acquisition (which are, in the satellite reference frame, the viewing incidence angles in the satellite X (resp. Y) directions, and the global viewing incidence (arctg of the quadratic sum of tan(X) and tan(Y)).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VE_Generic_Site_Product_Information_Type">
		<xs:annotation>
			<xs:documentation>generic public description of a site product</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_Generic_Product_Information_Type">
				<xs:sequence>
					<xs:element name="Reference_SiteDefinition_Id" type="ee:VE_SiteDefinition_Id_Type"/>
					<xs:element name="Cloud_Percentage">
						<xs:annotation>
							<xs:documentation>Percentage of cloudy pixels</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:union>
								<xs:simpleType>
									<xs:restriction base="ee:Decimal_Type">
										<xs:fractionDigits value="1"/>
									</xs:restriction>
								</xs:simpleType>
								<xs:simpleType>
									<xs:restriction base="ee:VE_Nil_Reason_NA_Type"/>
								</xs:simpleType>
							</xs:union>
						</xs:simpleType>
					</xs:element>
					<xs:element name="Used_DEM" type="ee:VE_DEM_Information_Type"/>
					<xs:element name="Reflectance_Quantification_Value">
						<xs:annotation>
							<xs:documentation>Reflectance corresponding to one bit of quantification (for example 1/1024). For system products the value is "NONE".</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:union>
								<xs:simpleType>
									<xs:restriction base="ee:Decimal_Type">
										<xs:fractionDigits value="6"/>
									</xs:restriction>
								</xs:simpleType>
								<xs:simpleType>
									<xs:restriction base="ee:VE_None_Type"/>
								</xs:simpleType>
							</xs:union>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VE_Generic_Product_Private_Information_Type">
		<xs:annotation>
			<xs:documentation>Private Data for all products (inventoriedRaw, system and L1)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Earth_Rotation_Parameters" type="ee:VE_Earth_Rotation_Parameters_Type">
				<xs:annotation>
					<xs:documentation>earth rotation parameters for the acquisition day (extracted from the IERS bulletin)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="List_of_Datation_Line_Models" type="ee:VE_List_of_Datation_Line_Models_Type">
				<xs:annotation>
					<xs:documentation>desctiption of each datation line model / band</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Cape" type="ee:Angle_Type">
				<xs:annotation>
					<xs:documentation>Cape (orientation), at the center of the image.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Attitude" type="ee:VE_Attitude_Type">
				<xs:annotation>
					<xs:documentation>Roll, pitch and yaw at the center of the acquisition</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Nadir" type="ee:Geo_Location_2D_Type">
				<xs:annotation>
					<xs:documentation>The latitude and longitude of the point of the ground track (at nadir) at the middle of the acquisition.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Geom_Slide_Angle" type="ee:Angle_Type">
				<xs:annotation>
					<xs:documentation>geometrical slip of ground pixels (orthogonality defect)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Along_Track_Resolution" type="ee:Distance_Type">
				<xs:annotation>
					<xs:documentation>Along track resolution at the center of the useful image (pixel size in meters)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Along_Detector_Resolution" type="ee:Distance_Type">
				<xs:annotation>
					<xs:documentation>Along detector resolution at the center of the useful image (pixel size in meters)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Acquisition_Tech_Params_Set" type="ee:VE_Acq_Tech_Params_Set_Type">
				<xs:annotation>
					<xs:documentation>Gain and Offset are expressed in camera referential, thus they have to be switched for bands 7 to 12 before to be used at image product</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="VE_Channel_Code_Type">
		<xs:annotation>
			<xs:documentation>All the codes for a channel</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ee:String_Type">
			<xs:pattern value="&ChannelCodeElt;"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VE_Annex_Band_Code_Type">
		<xs:annotation>
			<xs:documentation>All the codes for an annex channel</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="ee:VE_Band_Code_Type ee:VE_Channel_Code_Type ee:VE_Solar_Angle_Grid_Band_Code_Type ee:VE_Viewing_Angle_Grid_Band_Code_Type"/>
	</xs:simpleType>
	<xs:complexType name="VE_Minimum_Image_Information_Type">
		<xs:annotation>
			<xs:documentation>minimum description for an image</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Format">
				<xs:annotation>
					<xs:documentation>JPEG, TIFF, GEOTIFF, ...
HDF</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ee:String_Type">
						<xs:enumeration value="N/A"/>
						<xs:enumeration value="GEOTIFF"/>
						<xs:enumeration value="TIFF"/>
						<xs:enumeration value="JPEG"/>
						<xs:enumeration value="HDF"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Binary_Encoding" type="ee:VE_Binary_Encoding_Type">
				<xs:annotation>
					<xs:documentation>LITTLE_ENDIAN</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Data_Type">
				<xs:annotation>
					<xs:documentation>SIGNED_SHORT for products, UNSIGNED_SHORT for inventoriedRaw product,
UNSIGNED_BYTE for quickLooks</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ee:VE_Raster_Data_Type_Type"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Number_of_Significant_Bits" type="ee:Integer_Type"/>
			<xs:element name="Nodata_Value">
				<xs:annotation>
					<xs:documentation>Filler value expressed in integer or NaN</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:union memberTypes="ee:Integer_Type">
						<xs:simpleType>
							<xs:restriction base="ee:String_Type">
								<xs:pattern value="NaN"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:union>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Size">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Lines">
							<xs:annotation>
								<xs:documentation>Number of lines OR "NIL=VARIABLE_BY_BAND" for InventoriedRaw and system products main header.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:union memberTypes="ee:PositiveInteger_Type ee:VE_Nil_Reason_Variable_By_Band_Type"/>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Columns">
							<xs:simpleType>
								<xs:restriction base="ee:PositiveInteger_Type"/>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Bands">
							<xs:simpleType>
								<xs:restriction base="ee:Integer_Type">
									<xs:minInclusive value="0"/>
									<xs:maxInclusive value="15"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Image_Compacting_Tool">
				<xs:simpleType>
					<xs:restriction base="ee:String_Type">
						<xs:enumeration value="NO"/>
						<xs:enumeration value="BZIP2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VE_Generic_Image_Information_Type">
		<xs:annotation>
			<xs:documentation>generic description for an image</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_Minimum_Image_Information_Type">
				<xs:sequence>
					<xs:element name="List_of_Bands">
						<xs:annotation>
							<xs:documentation>Associated band (B01 to B12 for VENUS, B1 to B4 for FORMOSAT, etc..) or channel names (e.g. VAP, LAI, FAPAR,...).</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Band" maxOccurs="15">
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="ee:VE_Channel_Code_Type">
												<xs:attribute name="sn" use="required">
													<xs:annotation>
														<xs:documentation>Band or channel code in image file</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="ee:Integer_Type">
															<xs:minInclusive value="1"/>
															<xs:maxInclusive value="15"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="count" use="required">
								<xs:annotation>
									<xs:documentation>number of bands</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="ee:Integer_Type">
										<xs:minInclusive value="1"/>
										<xs:maxInclusive value="15"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--InventoriedRaw-->
	<xs:complexType name="VE_Inventoried_Product_Information_Type">
		<xs:annotation>
			<xs:documentation> description of a InventoriedRaw product</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_Generic_Product_Information_Type">
				<xs:sequence>
					<xs:element name="Coverage_Tag">
						<xs:simpleType>
							<xs:restriction base="ee:String_Type">
								<xs:enumeration value="NOMINAL"/>
								<xs:enumeration value="DATA_STRIP"/>
								<xs:enumeration value="UNKNOWN_SITE"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="List_of_Covered_Sites">
						<xs:annotation>
							<xs:documentation>list of the sites covered by the inventoried product</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence minOccurs="0" maxOccurs="unbounded">
								<xs:element name="Covered_Site">
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="ee:VE_SiteDefinition_Id_Type">
												<xs:attribute name="sn" type="ee:Integer_Type" use="required"/>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="count" type="ee:Integer_Type" use="required"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="Toponym" type="ee:String_Type">
						<xs:annotation>
							<xs:documentation>Toponym at the center of the inventoried product when it belongs to no site, or empty in the other case</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VE_Inventoried_Product_Organization_Type">
		<xs:annotation>
			<xs:documentation>Public organization of an inventoriedRawProduct</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="List_of_Image_Files">
				<xs:annotation>
					<xs:documentation>0-12 images for the inventoriedRaw and system products.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Image_File" minOccurs="0" maxOccurs="12">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Product_Item_Reference_Type">
										<xs:attribute name="sk" type="ee:VE_Band_Code_Type" use="required">
											<xs:annotation>
												<xs:documentation>image band code</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:annotation>
							<xs:documentation>= nb bands</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="12"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="List_of_Quicklook_Files">
				<xs:annotation>
					<xs:documentation>0-4 quicklook  files for the inventoriedRaw and system products : one per each tri-detector source file.
Set of detectors are : B02-B01-B05, B10-B12-B11, B07-B08-B09 and B04-B03-B06.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Quicklook_File" minOccurs="0" maxOccurs="4">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Product_Item_Reference_Type">
										<xs:attribute name="sk" type="ee:VE_Band_Code_Type" use="required">
											<xs:annotation>
												<xs:documentation>band code</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="4"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Private_Data_File" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ee:VE_Product_Item_Reference_Type"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="List_of_Extension_Data_Files" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Future extensions</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Extension_Data_File" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Product_Item_Reference_Type">
										<xs:attribute name="sn" use="required">
											<xs:simpleType>
												<xs:restriction base="ee:Integer_Type">
													<xs:minInclusive value="1"/>
													<xs:maxInclusive value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VE_Inventoried_Product_Private_Information_Type">
		<xs:annotation>
			<xs:documentation>Private technical Data of a product (inventoriedRaw, system and L1)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_Generic_Product_Private_Information_Type">
				<xs:sequence>
					<xs:element name="List_of_Line_Intervals" type="ee:VE_List_of_Line_Intervals_Type">
						<xs:annotation>
							<xs:documentation>The first and last lines of the greatest image which can be extracted or have been extracted (L1)  from the segment by band</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VE_Inventoried_Product_Private_Organization_Type">
		<xs:annotation>
			<xs:documentation>Private description of a system (and inventoriedRaw) product organization.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_System_Product_Private_Organization_Type">
				<xs:sequence>
					<xs:element name="List_of_Datation_Line_Files">
						<xs:annotation>
							<xs:documentation>desctiption of each datation line / band</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Datation_Line_File" minOccurs="0" maxOccurs="12">
									<xs:annotation>
										<xs:documentation>Reference to datation files</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="ee:VE_Product_Item_Reference_Type">
												<xs:attribute name="sk" type="ee:VE_Band_Code_Type" use="required">
													<xs:annotation>
														<xs:documentation>band code</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="count" use="required">
								<xs:simpleType>
									<xs:restriction base="xs:unsignedInt">
										<xs:minInclusive value="0"/>
										<xs:maxInclusive value="12"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--System products-->
	<xs:complexType name="VE_Sys_Product_Information_Type">
		<xs:annotation>
			<xs:documentation>system description of a product on one site</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_Generic_Product_Information_Type">
				<xs:sequence>
					<xs:element name="Reference_SiteDefinition_Id" type="ee:VE_SiteDefinition_Id_Type"/>
					<xs:element name="Cloud_Percentage">
						<xs:annotation>
							<xs:documentation>Percentage of cloudy pixels</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:union memberTypes="ee:Float_Type ee:VE_Nil_Reason_NA_Type"/>
						</xs:simpleType>
					</xs:element>
					<xs:element name="Used_DEM" type="ee:VE_DEM_Information_Type"/>
					<xs:element name="Used_Reference_Image">
						<xs:annotation>
							<xs:documentation>Logical name of the reference image used when exists (image on referenced site) ; string value for special L1 products ; else "NONE".</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:union memberTypes="ee:VE_ReferenceImageData_Id_Type ee:VE_None_Type"/>
						</xs:simpleType>
					</xs:element>
					<xs:element name="Solar_Angles" type="ee:VE_Solar_Angle_Information_Type">
						<xs:annotation>
							<xs:documentation>Information relative to solar angles.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="List_of_Viewing_Angles" type="ee:VE_List_of_Product_Viewing_Angles_Informations_Type">
						<xs:annotation>
							<xs:documentation>Information relative to viewing angles (one set for each tridetector).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Reflectance_Quantification_Value">
						<xs:annotation>
							<xs:documentation>Reflectance corresponding to one bit of quantification (for example 1/1024). For system products the value is "NONE".</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:union memberTypes="ee:Float_Type ee:VE_None_Type"/>
						</xs:simpleType>
					</xs:element>
					<xs:element name="List_of_Aks" type="ee:VE_List_of_Optionnal_Aks_Type">
						<xs:annotation>
							<xs:documentation>The absolute radiometric calibration coefficients (one per band)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Viewing_Angles_In_Satellite_Frame" type="ee:VE_Viewing_Angles_In_Satellite_Frame_Type">
						<xs:annotation>
							<xs:documentation>X,Y, and XY at the center of the acquisition (which are, in the satellite reference frame, the viewing incidence angles in the satellite X (resp. Y) directions, and the global viewing incidence (arctg of the quadratic sum of tan(X) and tan(Y)).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Processing_Flags_And_Modes">
						<xs:complexType>
							<xs:sequence>
								<xs:annotation>
									<xs:documentation>L1 products flags</xs:documentation>
								</xs:annotation>
								<xs:element name="Equalization" type="ee:Bool_Type"/>
								<xs:element name="Bad_Pixels_Correction" type="ee:Bool_Type"/>
								<xs:element name="Restoration" type="ee:Bool_Type"/>
								<xs:element name="Remanence" type="ee:Bool_Type"/>
								<xs:element name="Geometrical_Refinement_With_Ref_Image" type="ee:Bool_Type"/>
								<xs:element name="Interband_Geometrical_Refinement" type="ee:Bool_Type"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VE_System_Product_Information_Type">
		<xs:annotation>
			<xs:documentation>system description of a product on one site</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_Generic_Site_Product_Information_Type">
				<xs:sequence>
					<xs:element name="List_of_Aks" type="ee:VE_List_of_Optionnal_Aks_Type">
						<xs:annotation>
							<xs:documentation>The absolute radiometric calibration coefficients (one per band)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Processing_Flags_And_Modes">
						<xs:complexType>
							<xs:sequence>
								<xs:annotation>
									<xs:documentation>L1 products flags</xs:documentation>
								</xs:annotation>
								<xs:element name="Equalization" type="ee:Bool_Type"/>
								<xs:element name="Bad_Pixels_Correction" type="ee:Bool_Type"/>
								<xs:element name="Restoration" type="ee:Bool_Type"/>
								<xs:element name="Remanence" type="ee:Bool_Type"/>
								<xs:element name="Geometrical_Refinement_With_Ref_Image" type="ee:Bool_Type"/>
								<xs:element name="Interband_Geometrical_Refinement" type="ee:Bool_Type"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Used_Reference_Image">
						<xs:annotation>
							<xs:documentation>Logical name of the reference image used when exists (image on referenced site) ; string value for special L1 products (not valid) ; else "NONE".</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:union memberTypes="ee:VE_ReferenceImageData_Id_Type ee:VE_None_Type"/>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VE_System_Product_Organization_Type">
		<xs:annotation>
			<xs:documentation>Public description of a system (and inventoriedRaw) product organization.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="List_of_Image_Files">
				<xs:annotation>
					<xs:documentation>0-12 images for the inventoriedRaw and system products.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Image_File" minOccurs="0" maxOccurs="12">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Product_Item_Reference_Type">
										<xs:attribute name="sk" type="ee:VE_Band_Code_Type" use="required">
											<xs:annotation>
												<xs:documentation>image band code</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:annotation>
							<xs:documentation>= nb bands</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="12"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="List_of_Quicklook_Files">
				<xs:annotation>
					<xs:documentation>0-4 quicklook  files for the inventoriedRaw and system products : one per each tri-detector source file.
Set of detectors are : B02-B01-B05, B10-B12-B11, B07-B08-B09 and B04-B03-B06.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Quicklook_File" minOccurs="0" maxOccurs="4">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Product_Item_Reference_Type">
										<xs:attribute name="sk" type="ee:VE_Band_Code_Type" use="required">
											<xs:annotation>
												<xs:documentation>band code</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="4"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="List_of_Mask_Files" minOccurs="0">
				<xs:annotation>
					<xs:documentation>required only for L1A system products : adding 2 images = SAT (saturation) and PIX (missing and aberrant pixels) masks.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Mask_File" minOccurs="0" maxOccurs="2">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Product_Item_Reference_Type">
										<xs:attribute name="sk" type="ee:VE_PdtImg_Sub_Code_Type" use="required">
											<xs:annotation>
												<xs:documentation>mask code</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:annotation>
							<xs:documentation>= nb mask</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="2"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Private_Data_File" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ee:VE_Product_Item_Reference_Type"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="List_of_Extension_Data_Files" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Future extensions</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Extension_Data_File" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Product_Item_Reference_Type">
										<xs:attribute name="sn" use="required">
											<xs:simpleType>
												<xs:restriction base="ee:Integer_Type">
													<xs:minInclusive value="1"/>
													<xs:maxInclusive value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VE_System_Product_Private_Information_Type">
		<xs:annotation>
			<xs:documentation>Private technical Data of a system product</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ee:VE_Generic_Product_Private_Information_Type">
				<xs:sequence>
					<xs:element name="List_of_Line_Intervals" type="ee:VE_List_of_Line_Intervals_Type">
						<xs:annotation>
							<xs:documentation>The first and last line of the scene in the segment (in corresponding inventoried product), for each band</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Used_InventoriedRaw_Product_Id" type="ee:VE_InventoriedRawProduct_Id_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VE_System_Product_Private_Organization_Type">
		<xs:annotation>
			<xs:documentation>Private description of a system product organization.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="List_of_Auxiliary_Data_Files">
				<xs:annotation>
					<xs:documentation>Auxiliary data : GPS, AOCS, STR, ...
- Pointed in inventoriedRaw product
- Copied in system products private data</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Auxiliary_Data_File" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="ee:VE_Product_Item_Reference_Type">
										<xs:attribute name="sk" type="ee:VE_Auxiliary_Data_Code_Type" use="required">
											<xs:annotation>
												<xs:documentation>Identification code for an auxiliary data type (GPS, AOCS, STR, ...)</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="count" use="required">
						<xs:simpleType>
							<xs:restriction base="ee:Integer_Type">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Geom_Refinement_File" type="ee:VE_Product_Item_Reference_Type">
				<xs:annotation>
					<xs:documentation>Euclide geometrical model</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Published Products-->
	<xs:complexType name="VE_Published_Product_Organization_Type">
		<xs:annotation>
			<xs:documentation>Public description of organization of a published product.</xs:documentation>
