<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ee="http://eop-cfi.esa.int/CFI" targetNamespace="http://eop-cfi.esa.int/CFI" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.00">
	<xs:include schemaLocation="Dictionary.xsd"/>
	<xs:complexType name="VE_List_of_Product_Viewing_Angles_Informations_Type" mixed="false">
		<xs:annotation>
			<xs:documentation>All the viewing incidences informations (one for each tri-detector)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Viewing_Angles" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ee:VE_Product_Viewing_Angle_Information">
							<xs:attribute name="sn" use="required">
								<xs:annotation>
									<xs:documentation>tri-detector number</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="ee:Integer_Type">
										<xs:minInclusive value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="count" use="required">
			<xs:annotation>
				<xs:documentation>one for each tri-detector original files (=4)</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="ee:Integer_Type">
					<xs:minInclusive value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="VE_Product_Viewing_Angle_Information">
		<xs:choice>
			<xs:sequence>
				<xs:element name="Useful_Image">
					<xs:annotation>
						<xs:documentation>Viewing Angle in useful image corners and center</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Upper_Left_Corner" type="ee:VE_Decimal_Viewing_Angle_Type"/>
							<xs:element name="Upper_Right_Corner" type="ee:VE_Decimal_Viewing_Angle_Type"/>
							<xs:element name="Lower_Left_Corner" type="ee:VE_Decimal_Viewing_Angle_Type"/>
							<xs:element name="Lower_Right_Corner" type="ee:VE_Decimal_Viewing_Angle_Type"/>
							<xs:element name="Image_Center" type="ee:VE_Decimal_Viewing_Angle_Type"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Product" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Viewing Angle in product corners</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Upper_Left_Corner" type="ee:VE_Decimal_Viewing_Angle_Type"/>
							<xs:element name="Upper_Right_Corner" type="ee:VE_Decimal_Viewing_Angle_Type"/>
							<xs:element name="Lower_Left_Corner" type="ee:VE_Decimal_Viewing_Angle_Type"/>
							<xs:element name="Lower_Right_Corner" type="ee:VE_Decimal_Viewing_Angle_Type"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:element name="Nil" type="ee:VE_Nil_Reason_Type">
				<xs:annotation>
					<xs:documentation>When tri-detector data is MISSING for example</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
