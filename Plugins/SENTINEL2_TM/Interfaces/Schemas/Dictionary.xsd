    <xs:complexType name="VE_Image_Filter">
        <xs:annotation>
            <xs:documentation>Image filter definition</xs:documentation>
            <!--  <![CDATA[An
image filter is defined with :<br/>
					<ul>
					<li>its type (a textual
enumeration);</li>
					<li>the position of filter
center : this position has different meanings according to the file type
:<br>
						 <ul>
						 <li>  spectral filter
:position of the filter center </li>
						 <li>  spectral filter :
position of the continue coef in the filter </li>
						  <li>  wavelet filter :
position of the continue coef in the filter </li>
						  </ul>

					</li>
					<li>2 oversampling factors (row
wise, column wise) : usefull for geometrical transforms (resampling
filters for example);</li>
					<li>a data representation :
choice between </li>
						 <ul>
						 <li>  an array of
double for a bidim filter definition </li>
						 <li>  2 lists of
doubles representing the 2 1D-filters (row and column wise) if the image
filter is separable  </li>
						 </ul>	

					</ul>]]>-->
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Filter_Type" type="ee:VE_Image_Filter_Type"/>
            <xs:element name="Filter_Center" type="ee:VE_Position_in_Array"/>
            <xs:element name="Filter_Oversampling">
                <xs:annotation>
                    <xs:documentation>Oversampling between Filter and Image</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Row_Wise" type="xs:int">
                            <xs:annotation>
                                <xs:documentation>Filter tabulated at 1/ROW_WISE image pixels along
                                    the rows</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Col_Wise" type="xs:int">
                            <xs:annotation>
                                <xs:documentation>Filter tabulated at 1/COL_WISE image pixels along
                                    the col</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Filter_Data">
                <xs:complexType>
                    <xs:choice>
                        <xs:element name="Bidim_Filter_Data" type="ee:VE_Array_of_Double"/>
                        <xs:element name="Separable_Filter_Data">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Nb_of_Rows" type="xs:int"/>
                                    <xs:element name="Nb_of_Cols" type="xs:int"/>
                                    <xs:element name="Origin" type="xs:int">
                                        <xs:annotation>
                                            <xs:documentation>array count start</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Row_Coefs" type="ee:VE_List_of_Double">
                                        <xs:annotation>
                                            <xs:documentation>coefficients for the row wise 1D
                                                filter</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Col_Coefs" type="ee:VE_List_of_Double">
                                        <xs:annotation>
                                            <xs:documentation>coefficients for the column wise 1D
                                                filter</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="VE_List_of_Double">
        <xs:annotation>
            <xs:documentation>An unbounded list of doubles.</xs:documentation>
        </xs:annotation>
        <xs:list itemType="xs:double"/>
    </xs:simpleType>
    <xs:simpleType name="VE_List_of_Int">
        <xs:annotation>
            <xs:documentation>List of int numbers</xs:documentation>
        </xs:annotation>
        <xs:list itemType="xs:int"/>
    </xs:simpleType>
    <xs:complexType name="VE_Position_in_Array">
        <xs:annotation>
            <xs:documentation>Describes a position in an array (raw_index, column_index) The position in an array must be coherent with the origin of the
                array (count element start)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Row_Index" type="xs:positiveInteger"/>
            <xs:element name="Col_Index" type="xs:positiveInteger"/>
        </xs:sequence>
    </xs:complexType>
    <!--DEM-->
    <xs:simpleType name="VE_DEM_SRTM_Reference_Type">
        <xs:annotation>
            <xs:documentation>Pattern : SRTM=DEM_id </xs:documentation>
        </xs:annotation>
        <xs:restriction base="ee:String_Type">
            <xs:pattern value="SRTM=([0-9A-Z]{0,40})"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="VE_DEM_LOCAL_Reference_Type">
        <xs:annotation>
            <xs:documentation>Pattern : LOCAL=DEM_id </xs:documentation>
        </xs:annotation>
        <xs:restriction base="ee:String_Type">
            <xs:pattern value="LOCAL=&SatelliteClassGeneric;_&FileClass;_AUX_REFDE2_&NickNameElt;_&Version4Elt;"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="VE_DEM_GLOBAL_Reference_Type">
        <xs:annotation>
            <xs:documentation>Pattern : GLOBAL=name</xs:documentation>
        </xs:annotation>
        <xs:restriction base="ee:String_Type">
            <xs:pattern value="GLOBAL=([0-9A-Z]{0,40})"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="VE_DEM_Information_Type">
        <xs:annotation>
            <xs:documentation>DEM description</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="DEM_Reference">
                <xs:annotation>
                    <xs:documentation>DEM logical name when exists (image on referenced site) or global DEM.
- SRTM=id
- LOCAL=id
- GLOBAL=name
NONE if no dem used.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:union memberTypes="ee:VE_DEM_LOCAL_Reference_Type ee:VE_DEM_SRTM_Reference_Type ee:VE_DEM_GLOBAL_Reference_Type ee:VE_None_Type"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Statistics" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Statistics of the DEM over the image</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Min" type="ee:Distance_Type"/>
                        <xs:element name="Max" type="ee:Distance_Type"/>
                        <xs:element name="Average" type="ee:Distance_Type"/>
                        <xs:element name="Standard_Deviation" type="ee:Distance_Type"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
