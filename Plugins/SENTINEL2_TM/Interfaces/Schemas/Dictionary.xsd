        <xs:restriction base="ee:String_Type">
            <xs:pattern value="&BandCodeElt;_(DX|DY)_ALT=[0-9]{1,5}.[0-9]{0,2}m"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="VE_Area_Code_Type">
        <xs:annotation>
